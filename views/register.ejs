<%- include('header'); -%>
<div class="content">
  <h1>Registration</h1>
  <form action="/api/register" method="post">
    <label for="username">Username:</label>
    <input type="text" id="username" name="username" placeholder="Username" />
    <br />
    <label for="password">Password:</label>
    <input
      type="password"
      id="password"
      name="password"
      placeholder="Password"
    />
    <br />
    <label for="name">Name:</label>
    <input type="text" id="name" name="name" placeholder="Full Name" />
    <br />
    <label for="society">Society:</label>
    <input type="text" id="society" name="society" />
    <br />
    <label for="contact">Contact:</label>
    <input type="tel" id="contact" name="contact" />
    <br />
    <label for="addres">Address:</label>
    <input type="text" id="addres" name="addres" />
    <br />
    <label for="zipcode">Zipcode</label>
    <input type="number" id="zipcode" name="zipcode" />
    <br />
    <label for="city">City:</label>
    <input type="text" id="city" name="city" placeholder="City" />
    <br />
    <label for="phone">Phone:</label>
    <input type="text" id="phone" name="phone" />
    <br />
    <label for="fax">Fax:</label>
    <input type="number" id="fax" name="fax" />
    <br />
    <label for="confirmation">Confirmation:</label>
    <input type="text" id="confirmation" name="confirmation" />
    <br />
    <label for="payment">Payment:</label>
    <input type="text" id="payment" name="payment" />
    <br />
    <label for="Informations">Informations:</label>
    <input type="text" id="Informations" name="Informations" />
    <br />
    <label for="max_outstanding">Max Outstanding:</label>
    <input type="text" id="max_outstanding" name="max_outstanding" />
    <br />
    <button type="submit">Register</button>
  </form>
</div>
<br />
<%- include('footer'); -%>
